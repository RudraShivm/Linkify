
CREATE DATABASE LINKIFY;
USE LINKIFY;



CREATE TABLE City(
	name VARCHAR(100) DEFAULT '',
	PRIMARY KEY(name)
); 


CREATE TABLE Customer (
	id INT AUTO_INCREMENT,
    profile_picture VARCHAR(500),
    store_name VARCHAR(200) DEFAULT '',
    city VARCHAR(100),
    mobile_no VARCHAR(11),
    passwords VARCHAR(100),
    owner_name VARCHAR(100) DEFAULT '',
    owner_nid VARCHAR(12) DEFAULT 'XXX-XXX-XXXX',
    PRIMARY KEY(id),
	FOREIGN KEY(city) REFERENCES City(name),
    CHECK (mobile_no LIKE '01%')
)AUTO_INCREMENT=1210000001;




CREATE TABLE Warehouse(
	id INT,
    city VARCHAR(100) DEFAULT '',
    PRIMARY KEY(id),
    FOREIGN KEY(city) REFERENCES City(name)
);
-- 1220000001



CREATE TABLE Warehouse_mgr(
	id INT AUTO_INCREMENT,
    name VARCHAR(200),
    designation VARCHAR(30),
    profile_picture VARCHAR(500),
    nid VARCHAR(15) DEFAULT 'XXX-XXX-XXXX',
    mobile_no VARCHAR(11),
    passwords VARCHAR(100),
    joining_date DATE NOT NULL,
    salary INT DEFAULT 0,
    warehouse_id INT,
    PRIMARY KEY(id),
    FOREIGN KEY(warehouse_id) REFERENCES Warehouse(id),
	CHECK (mobile_no LIKE '01%')
)AUTO_INCREMENT=1230000001;




CREATE TABLE Factory(
	id INT AUTO_INCREMENT,
    city VARCHAR(100),
    PRIMARY KEY(id),
    FOREIGN KEY(city) REFERENCES City(name)
)AUTO_INCREMENT=1240000001;




CREATE TABLE Production_mgr(
	id INT AUTO_INCREMENT,
    name VARCHAR(200),
    designation VARCHAR(30),
    profile_picture VARCHAR(500),
    nid VARCHAR(12) DEFAULT 'XXX-XXX-XXXX',
    mobile_no VARCHAR(11),
    passwords VARCHAR(100),
    joining_date DATE NOT NULL,
    salary INT DEFAULT 0,
    factory_id INT,
    PRIMARY KEY(id),
	FOREIGN KEY(factory_id) REFERENCES Factory(id),
	CHECK (mobile_no LIKE '01%')
)AUTO_INCREMENT=1250000001;



CREATE TABLE Delivery_mgr(
	id INT AUTO_INCREMENT,
    name VARCHAR(200),
    designation VARCHAR(30),
    profile_picture VARCHAR(500),
    nid VARCHAR(15) DEFAULT 'XXX-XXX-XXXX',
    mobile_no VARCHAR(11),
    passwords VARCHAR(100),
    joining_date DATE NOT NULL,
    salary INT DEFAULT 0,
    branch_city VARCHAR(100),
    FOREIGN KEY(branch_city) REFERENCES City(name),
    PRIMARY KEY(id),
	CHECK (mobile_no LIKE '01%')
)AUTO_INCREMENT=1260000001;





CREATE TABLE Product (
    id INT ,
    name VARCHAR(100),
    model VARCHAR(100),
    series VARCHAR(100),
    description VARCHAR(1500),
    display VARCHAR(100),
    picture1 VARCHAR(500),
    picture2 VARCHAR(500),
    picture3 VARCHAR(500),
    unit_price INT,
    minimum_delivery_time INT,
    PRIMARY KEY (id)
) ;
-- 1270000001



CREATE TABLE Orders(
	id INT AUTO_INCREMENT,
    sub_id INT,
    product_id INT,
    customer_id INT,
    warehouse_mgr_id INT,
    qty INT DEFAULT 0,
	order_place_date DATE NOT NULL,
    exp_delivery_date DATE,
    paid_amount INT,
    due_amount INT,
    status VARCHAR(20) DEFAULT 'processing',
    PRIMARY KEY(id,sub_id),
    FOREIGN KEY(customer_id) REFERENCES Customer(id),
	FOREIGN KEY(warehouse_mgr_id) REFERENCES Warehouse_mgr(id),
    FOREIGN KEY(product_id) REFERENCES Product(id)
)AUTO_INCREMENT=1280000001;





CREATE TABLE Feedback(
	id INT AUTO_INCREMENT,
    customer_id INT,
    order_id INT,
    stars INT,
    PRIMARY KEY(id),
    FOREIGN KEY(customer_id) REFERENCES Customer(id),
    FOREIGN KEY(order_id) REFERENCES Orders(id),
    CHECK (stars>=0 AND stars<=5)
)AUTO_INCREMENT=1290000001;



CREATE TABLE Warehouse_stock (
    id INT AUTO_INCREMENT PRIMARY KEY,
    warehouse_id INT,
    product_id INT,
    available_qty INT,
    FOREIGN KEY(warehouse_id) REFERENCES Warehouse(id),
    FOREIGN KEY(product_id) REFERENCES Product(id)
) AUTO_INCREMENT=1310000001;




CREATE TABLE Ware_request(
	id INt AUTO_INCREMENT,
	warehouse_id INT,
    production_mgr_id INT,
    ware_stock_id INT,
    request_date DATE NOT NULL,
    qty INT DEFAULT 0,
    status VARCHAR(20) DEFAULT 'Pending',
    PRIMARY KEY(id),
	FOREIGN KEY(warehouse_id) REFERENCES Warehouse(id),
	FOREIGN KEY(ware_stock_id) REFERENCES Warehouse_stock(id),
    FOREIGN KEY(production_mgr_id) REFERENCES Production_mgr(id)
)AUTO_INCREMENT=1320000001;




CREATE TABLE Factory_stock(
	id INT AUTO_INCREMENT primary key,
	factory_id INT,
	available_qty INT,
    last_added_date DATE NOT NULL,
    product_id INT,
    FOREIGN KEY(factory_id) REFERENCES Factory(id),
	FOREIGN KEY(product_id) REFERENCES Product(id)
)AUTO_INCREMENT=1330000001;



CREATE TABLE Ware_invoice(
	id INT AUTO_INCREMENT,
    ware_stock_id INT,
    ware_req_id INT,
    delivery_date DATE NOT NULL,
    factory_stock_id INT,
    PRIMARY KEY(id),
    FOREIGN KEY(ware_stock_id) REFERENCES Warehouse_stock(id),
    FOREIGN KEY(ware_req_id) REFERENCES Ware_request(id),
    FOREIGN KEY(factory_stock_id) REFERENCES Factory_stock(id)
)AUTO_INCREMENT=1340000001;




CREATE TABLE Raw_material(
	id INT AUTO_INCREMENT,
    name VARCHAR(100),
    unit_price INT DEFAULT 0,
    PRIMARY KEY(id),
    company VARCHAR(100),
    type VARCHAR (50)
)AUTO_INCREMENT=1350000001;




CREATE TABLE Factory_raw_stock(
	id INT AUTO_INCREMENT unique,
	product_id INT,
    raw_mat_id INT,
    FOREIGN KEY(product_id) REFERENCES Product(id),
    FOREIGN KEY(raw_mat_id) REFERENCES Raw_material(id)
)AUTO_INCREMENT=1360000001;	


CREATE TABLE  Supplier(
	id INT AUTO_INCREMENT,
    store_name VARCHAR(100) DEFAULT '',
	owner_name VARCHAR(100) DEFAULT '',
    profile_picture VARCHAR(500),
    owner_nid VARCHAR(12) DEFAULT 'XXX-XXX-XXXX',
    mobile_no VARCHAR(11),
    raw_mat_id INT,
    PRIMARY KEY(id),
    FOREIGN KEY(raw_mat_id) REFERENCES Raw_material(id),
    CHECK (mobile_no LIKE '01%')
)AUTO_INCREMENT=1370000001;




CREATE TABLE Factory_request(
	id INT AUTO_INCREMENT,
    req_date DATE NOT NULL,
    production_mgr_id INT,
    factory_r_stock_id INT,
    supplier_id INT,
    status VARCHAR(20) DEFAULT 'Pending',
    PRIMARY KEY(id),
    FOREIGN KEY(factory_r_stock_id) REFERENCES Factory_raw_stock(id),
    FOREIGN KEY(production_mgr_id) REFERENCES Production_mgr(id),
    FOREIGN KEY(supplier_id) REFERENCES Supplier(id)
)AUTO_INCREMENT=1380000001;




CREATE TABLE Factory_invoice(
	id INT AUTO_INCREMENT,
    delivery_date DATE NOT NULL,
    factory_req_id INT,
    factory_r_stock_id INT,
    PRIMARY KEY(id),
    FOREIGN KEY(factory_req_id) REFERENCES Factory_request(id),
    FOREIGN KEY(factory_r_stock_id) REFERENCES Factory_raw_stock(id)
)AUTO_INCREMENT=1390000001;


CREATE TABLE Delivery(
	id INT AUTO_INCREMENT,
	order_id INT ,
    delivery_mgr_id INT	,
    delivery_time DATE,
    PRIMARY KEY(id),
    FOREIGN KEY(delivery_mgr_id) REFERENCES Delivery_mgr(id),
    FOREIGN KEY(order_id) REFERENCES Orders(id)
)AUTO_INCREMENT=1420000001;


CREATE TABLE Customer_invoice(
  id INT AUTO_INCREMENT,
  order_id INT,
  order_sub_id INT,
  qty INT,
  ware_stock_id INT,
  delivery_id INT,
  product_id INT,
  PRIMARY KEY(id),
  -- FOREIGN KEY(order_id) REFERENCES Orders(id),
  -- FOREIGN KEY(order_sub_id) REFERENCES Orders(sub_id),
  FOREIGN KEY(order_id, order_sub_id) REFERENCES Orders(id, sub_id),
  FOREIGN KEY(ware_stock_id) REFERENCES Warehouse_stock(id),
  FOREIGN KEY(product_id) REFERENCES Product(id),
  FOREIGN KEY(delivery_id) REFERENCES Delivery(id)
) AUTO_INCREMENT=1410000001;








CREATE TABLE Supply_mgr(
	id INT AUTO_INCREMENT,
    name VARCHAR(200),
    designation VARCHAR(30),
    profile_picture VARCHAR(500),
    nid VARCHAR(12) DEFAULT 'XXX-XXX-XXXX',
    mobile_no VARCHAR(11),
    passwords VARCHAR(100),
    joining_date DATE NOT NULL,
    salary INT DEFAULT 0,
    factory_id INT,
    PRIMARY KEY(id),
	FOREIGN KEY(factory_id) REFERENCES Factory(id),
	CHECK (mobile_no LIKE '01%')
)AUTO_INCREMENT=1430000001;




